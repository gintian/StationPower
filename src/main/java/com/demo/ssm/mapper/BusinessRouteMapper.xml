<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.ssm.mapper.BusinessRouteMapper" >

    <!--结果映射-->
    <resultMap id="RouteMap" type="com.demo.ssm.po.BusinessRoute">
        <id column="ID" property="id" jdbcType="INTEGER"/>
        <result column="name1" property="stationName1" jdbcType="VARCHAR"/>
        <result column="name2" property="stationName2" jdbcType="VARCHAR"/>
        <result column="fiberOcc" property="fiberOcc" jdbcType="VARCHAR"/>

    </resultMap>

    <!--字段片段-->
    <sql id="Route">
        ID,name1,name2,fiberOcc
    </sql>

    <!--按照ID查询-->
    <select id="selectByPrimaryKey" resultMap="RouteMap" >
        SELECT *
        FROM BusinessRouteAssignment

    </select>

    <select id="count" resultType="java.lang.Integer">
        SELECT COUNT(id)
        FROM BusinessRouteAssignment

    </select>

</mapper>