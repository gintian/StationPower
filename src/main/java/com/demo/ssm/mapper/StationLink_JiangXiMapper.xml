<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.ssm.mapper.StationLink_JiangXiMapper" >

    <!--结果映射-->
    <resultMap id="LinkMap" type="com.demo.ssm.po.StationLink_JiangXi">
        <id column="ID" property="id" jdbcType="INTEGER"/>
        <result column="name1" property="name1" jdbcType="VARCHAR"/>
        <result column="name2" property="name2" jdbcType="VARCHAR"/>
        <result column="fiberOcc" property="fiberOcc" jdbcType="VARCHAR"/>

    </resultMap>

    <!--字段片段-->
    <sql id="Link">
        ID,name1,name2,fiberOcc
    </sql>

    <!--按照ID查询-->
    <select id="selectByPrimaryKey" resultMap="LinkMap" >
        SELECT *
        FROM StationLink_jiangxi
    </select>

    <select id="count" resultType="java.lang.Integer">
        SELECT COUNT(id)
        FROM StationLink_jiangxi

    </select>

</mapper>